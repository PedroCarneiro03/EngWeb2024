extends layout

block content
    .w3-card-4
        header.w3-container.w3-pink
            h1= titulo
        .w3-container
            p Id: #{compositor.id}
            p Nome: #{compositor.nome}
            p Biografia: #{compositor.bio}
            p Data Nascimento: #{compositor.dataNasc}
            p Data Obito: #{compositor.dataObito}
            p Periodo: #{compositor.periodo}
        footer.w3-container.w3-pink
            address Gerado por EngWeb2024 em #{data}
            a(href="/compositores") Voltar 
